<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="275.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StartScreen">
   <children>
      <VBox alignment="CENTER" layoutX="1.0" prefHeight="275.0" prefWidth="330.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label text="Welcome to the West End Pub &amp; Grill" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="tableButton" mnemonicParsing="false" onAction="#goToTable" text="Press Here To Start" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
